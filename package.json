{
  "name": "codegen-cloner",
  "version": "1.2.1",
  "description": "CLI tool to clone and setup PR projects from Codegen",
  "main": "dist/index.js",
  "bin": {
    "codegen-cloner": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "build:binaries": "npm run build && pkg dist/index.js --out-path binaries",
    "build:all": "npm run build && pkg . --targets node18-linux-x64,node18-macos-x64,node18-macos-arm64,node18-win-x64 --out-path binaries"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "ora": "^5.4.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/prompts": "^2.4.9",
    "pkg": "^5.8.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "pkg": {
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-macos-arm64",
      "node18-win-x64"
    ],
    "outputPath": "binaries",
    "scripts": [
      "dist/**/*.js"
    ]
  },
  "keywords": [
    "cli",
    "github",
    "pr",
    "docker",
    "pnpm"
  ],
  "author": "",
  "license": "MIT"
}
